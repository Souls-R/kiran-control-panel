[Desktop Entry]
#插件名
Name=Group Manager
Name[zh_CN]=用户组管理工具
Comment=Group Manager
Comment[zh_CN]=用户组管理工具
Terminal=false
Type=Application
Exec=kiran-cpanel-launcher --cpanel-plugin=kiran-cpanel-group
Icon=kcp-group
StartupNotify=true
Categories=Qt;Settings;Group;X-System-Server;X-Common-Features;
X-KIRAN-NoDisplay=true

[Kiran Control Panel Plugin]
#插件排序
Weight=998
#插件分类
Category=group-management
#插件共享库位置，若以'/'开头则为绝对路径，也可支持通过共享库名去插件安装目录去查找
Library=lib@PROJECT_NAME@.so
SubItems=Group

[Group]
#插件子项名 该值提供给控制中心形成子项列表
Name=group
Name[zh_CN]=用户组
#插件Icon
Icon=kcp-group
#插件关键字 该值提供给控制中心形成搜索列表
Keywords[zh_CN]=用户组管理

