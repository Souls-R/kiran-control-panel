[Desktop Entry]
Name=Network Settings
Name[zh_CN]=网络设置
Comment=Kiran Cpanel Network
Comment[zh_CN]=Kiran网络设置
#插件图标,同时也是快捷方式图标
Icon=kcp-network
# 配置通过启动器单独执行，需传入插件的desktop文件名，启动器获取该dekstop文件位置加载相关信息
# <kiran-cpanel-launcher -cpanel-plugin 插件的desktop文件名>
Exec=kiran-cpanel-launcher --cpanel-plugin=kiran-cpanel-network
Categories=Network;Settings;HardwareSettings;hardware;
Keywords=mate-control-center;Kiran;network;settings;
Terminal=false
Type=Application
X-KIRAN-NoDisplay=true

#插件信息存储的组（Group)
[Kiran Control Panel Plugin]
#插件排序
Weight=3
#插件分类
Category=network
#插件共享库位置，若以'/'开头则为绝对路径，也可支持通过共享库名去插件安装目录去查找
Library=libkiran-cpanel-network.so
#功能子项，控制面板会根据该值对下列的功能子项Group进行解析
SubItems=Network

#功能子项名
[Network]
#功能子项名 该值提供给控制中心形成功能子项列表
Name=network
Name[zh_CN]=网络选项
#插件Icon
Icon=kcp-network
#插件关键字 该值提供给控制中心形成搜索列表
Keywords[zh_CN]=网络设置
