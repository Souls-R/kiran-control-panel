[Desktop Entry]
Name=Power Manager
Name[zh_CN]=电源管理
Comment=Kiran Desktop Power Manager
Comment[zh_CN]=Kiran桌面电源管理
Terminal=false
Type=Application
#通过启动器单独执行 <kiran-cpanel-launcher -cpanel-plugin 插件的desktop文件名>
Exec=kiran-cpanel-launcher --cpanel-plugin=kiran-cpanel-power
Icon=kcp-power
StartupNotify=true
Categories=Settings;HardwareSettings;X-System-Server;
Keywords=mate-control-center;;Kiran;power;management;preferences;settings;
X-KIRAN-NoDisplay=true

[Kiran Control Panel Plugin]
Weight=99
Category=power-management
Library=lib@PROJECT_NAME@.so
SubItems=GeneralSettings,PowerSettings,BatterySettings

[GeneralSettings]
Name=General Settings
Name[zh_CN]=通用设置
Icon=kcp-power-general-settings
Keywords=General Settings
Keywords[zh_CN]=通用设置

[PowerSettings]
Name=Power Settings
Name[zh_CN]=电源设置
Icon=kcp-power-power-settings
Keywords=Power Settings
Keywords[zh_CN]=电源设置

[BatterySettings]
Name=Battery Settings
Name[zh_CN]=电池设置
Icon=kcp-power-battery-settings
Keywords=Battery Settings
Keywords[zh_CN]=电池设置


