[Desktop Entry]
Name=Audio Settings
Name[zh_CN]=音频设置
Comment=Kiran Cpanel Audio
Comment[zh_CN]=Kiran音频设置
#插件图标,同时也是快捷方式图标
Icon=kcp-audio
# 配置通过启动器单独执行，需传入插件的desktop文件名，启动器获取该dekstop文件位置加载相关信息
# <kiran-cpanel-launcher -cpanel-plugin 插件的desktop文件名>
Exec=kiran-cpanel-launcher --cpanel-plugin=kiran-cpanel-audio
Categories=Audio;Settings;HardwareSettings;hardware;
Keywords=mate-control-center;Kiran;audio;settings;
Terminal=false
Type=Application
X-KIRAN-NoDisplay=true

#插件信息存储的组（Group)
[Kiran Control Panel Plugin]
#插件排序
Weight=3
#插件分类
Category=hardware
#插件共享库位置，若以'/'开头则为绝对路径，也可支持通过共享库名去插件安装目录去查找
Library=libkiran-cpanel-audio.so
#功能子项，控制面板会根据该值对下列的功能子项Group进行解析
SubItems=OutputPage,InputPage

#功能子项名
[OutputPage]
#功能子项名 该值提供给控制中心形成功能子项列表
Name[zh_CN]=输出
#插件Icon
Icon=kcp-audio-output
#插件关键字 该值提供给控制中心形成搜索列表
Keywords=Output
Keywords[zh_CN]=输出

[InputPage]
Name[zh_CN]=输入
Icon=kcp-audio-input
Keywords=Input
Keywords[zh_CN]=输入