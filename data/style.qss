/****************QRadioButton************/
QRadioButton
{

    color:#ffffff;
    spacing:12px;
    outline: none;
}

QRadioButton::indicator {
    width: 14px;
    height: 14px;
}

QRadioButton::indicator::unchecked {
    image: url(:/images/unchecked.svg);
}


QRadioButton::indicator::checked {
    image: url(:/images/checked.svg);
}

