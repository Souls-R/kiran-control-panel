#!/usr/bin/env xdg-open
[Desktop Entry]
Encoding=UTF-8
Name=Kiran CPanel Appearance
Name[zh_CN]=个性化
Comment=Kiran CPanel Appearance
Comment[zh_CN]=个性化设置
Exec=kiran-cpanel-launcher --cpanel-plugin=kiran-cpanel-appearance
Icon=@ICON_DIR@/appearance.png
Terminal=false
Type=Application
Categories=GTK;Settings;DesktopSettings;X-Individuation;X-Common-Features;
StartupNotify=true
X-KIRAN-NoDisplay=true


[Kiran Control Panel Plugin]
#插件排序
Weight=1
#插件分类
Category=individuation
#插件共享库位置，若以'/'开头则为绝对路径，也可支持通过共享库名去插件安装目录去查找
Library=@CPANEL_PLUGIN_DIR@/lib@PROJECT_NAME@.so
SubItems=Wallpaper,Themes,Fonts

[Wallpaper]
#插件子项名 该值提供给控制中心形成子项列表
Name=Wallpaper
Name[zh_CN]=壁纸设置
#插件Icon
Icon=@ICON_DIR@/wallpaper.svg
#插件关键字 该值提供给控制中心形成搜索列表
Keywords[zh_CN]=壁纸,个性化 
Keywords=wallpaper,appearance

[Themes]
#插件子项名 该值提供给控制中心形成子项列表
Name=Theme
Name[zh_CN]=主题设置
#插件Icon
Icon=@ICON_DIR@/themes.svg
#插件关键字 该值提供给控制中心形成搜索列表
Keywords[zh_CN]=主题,个性化
Keywords=theme,appearance

[Fonts]
#插件子项名 该值提供给控制中心形成子项列表
Name=Font
Name[zh_CN]=字体设置
#插件Icon
Icon=@ICON_DIR@/font.svg
#插件关键字 该值提供给控制中心形成搜索列表
Keywords[zh_CN]=字体,个性化
Keywords=font,appearance

